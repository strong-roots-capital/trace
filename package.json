{
  "name": "@strong-roots-capital/trace",
  "version": "1.2.0",
  "description": "tap function to log a value with optional tag",
  "keywords": [
    "tap",
    "trace"
  ],
  "homepage": "https://github.com/strong-roots-capital/trace",
  "bugs": {
    "url": "https://github.com/strong-roots-capital/trace/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strong-roots-capital/trace.git"
  },
  "license": "ISC",
  "author": "Eric Crosson <eric.s.crosson@utexas.edu> (https://github.com/EricCrosson)",
  "main": "dist/src/trace.js",
  "types": "dist/src/trace.d.ts",
  "files": [
    "dist/src/"
  ],
  "scripts": {
    "ava": "ava -v",
    "build": "tsc",
    "clean": "rm -rf -- dist",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "format:check": "prettier --list-different --ignore-unknown .",
    "format:write": "prettier --write --ignore-unknown .",
    "lint": "eslint --ext ts .",
    "lint:fix": "eslint --ext ts --fix .",
    "test": "nyc ava"
  },
  "dependencies": {},
  "devDependencies": {
    "@ericcrosson/eslint-config": "3.0.0",
    "@ericcrosson/prettier-config": "1.0.0",
    "@semantic-release/git": "10.0.1",
    "@types/node": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "ava": "^3.4.0",
    "codecov": "^3.6.5",
    "eslint": "^8.0.0",
    "nyc": "^15.0.0",
    "prettier": "1.19.1",
    "semantic-release": "19.0.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "!dist/**/*.d.ts",
      "test/**/test-*.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
